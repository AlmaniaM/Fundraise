@using Fundraise.MvcExample.Models
@model ProfileViewModel
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="page-header">
    <h1>Profile</h1>
    <h2>Donations</h2>
</div>
<div class="row">
    <ul>
        @foreach (var donation in Model.Donations)
        {
            <li>@donation.Amount</li>
        }
    </ul>

</div>
